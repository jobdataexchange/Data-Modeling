schema:unitText a rdfs:Property ;
    dct:identifier "schema:unitText"@en-US ;
    rdfs:label "Unit Text"@en-US ;    
    rdfs:comment "Unit of measurement on which employee pay is based such as \"hour\", \"day\", \"week\", \"month\", \"year\"; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain schema:QuantitativeValue ;
    rdfs:range skos:Concept  ;
    vann:usageNote "Google Jobs recommends using \"HOUR\", \"DAY\", \"WEEK\", \"MONTH\", \"YEAR\"; these values also appear in HR Open's PayCycleIntervalCodeList.json"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:validFrom a rdfs:Property ;
    dct:identifier "schema:validThrough"@en-US ;
    rdfs:label "Valid Through"@en-US ;    
    rdfs:comment "The date from which the item is not valid."@en-US ;
    dct:description: "The date from which the job posting is valid in ISO 8601 format. For example, \"2017-02-24\" or \"2017-02-24T19:33:17+00:00\" (Google) ."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:JobPosting ;
    rdfs:range xsd:dateTime ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:validThrough a rdfs:Property ;
    dct:identifier "schema:validThrough"@en-US ;
    rdfs:label "Valid Through"@en-US ;    
    rdfs:comment "The date after when [which?] the item is not valid. For example the end of an offer, salary period, or a period of opening hours."@en-US ;
    dct:description: "The date when the job posting will expire in ISO 8601 format. For example, \"2017-02-24\" or \"2017-02-24T19:33:17+00:00\" (Google) ."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:JobPosting ;
    rdfs:range xsd:dateTime ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:value a rdfs:Property ;
    dct:identifier "schema:value"@en-US ;
