@prefix dct: <http://purl.org/dc/terms/> .
@prefix jdx: <http://purl.org/jdx/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ceasn: <http://purl.org/ctdlasn/terms/> .
@prefix ceterms: <http://purl.org/ctdl/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


jdx:AnnotatedDefinedTerm a rdfs:Class ; ;
    dct:identifier "jdx:AnnotatedDefinedTerm"@en-US ;
    rdfs:comment "A word, name, acronym, phrase, defined in a controlled vocabulary (e.g., taxonomy, glossary, dictionary, tag list, and other forms of enumeration)."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Annotated Term"@en-US ;
    vann:usageNote "Used in the context of JDX to assign terms from occupation, industry and instructional program classifications, skills and abilities, and abstract concepts to JDX entities. Through the class' relationship with the jdx:ScaledAnnotation class, terms can be conditioned through applications of scales for attributes such as importance and frequency."@en-US .
    
schema:ContactPoint a rdfs:Class ; ;
    dct:identifier "schema:ContactPoint"@en-US ;
    rdfs:comment "A contact point—e.g., for information about a job opening or interview."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Contact Point"@en-US ;
    vann:usageNote "@@@"@en-US .
    
jdx:Credential a rdfs:Class ; ;
    dct:identifier "jdx:Credential"@en-US ;
    rdfs:comment "Authoritative permission to hold a certain status or to do certain things, e.g. to practise some trade or profession."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Credential"@en-US ;
    vann:usageNote "@@@"@en-US .
    
jdx:Education a rdfs:Class ; ;
    dct:identifier "jdx:Education"@en-US ;
    rdfs:comment "Entity describing acquired knowledge, skills, values, beliefs and habits through systematic instruction and study that is required or recommended for a job position."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Education"@en-US ;
    vann:usageNote "@@@"@en-US .
    
jdx:Experience a rdfs:Class ; ;
    dct:identifier "jdx:Experience"@en-US ;
    rdfs:comment "Entity describing past individual performance confirming or supporting qualification to engage in defined activities, perform certain tasks or to assume certain responsibilities required or recommended for a job position."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Experience"@en-US ;
    vann:usageNote "@@@"@en-US .    
  
schema:GeoCoordinates a rdfs:Class ; ;
    dct:identifier "schema:GeoCoordinates"@en-US ;
    rdfs:comment "Geographic coordinates of a place or event."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Location Geocoordinates"@en-US ;
    vann:usageNote "Use to identify a place using geo coordinates; preferably includes a URI for the location as well as longitude and latitude"@en-US .
    
jdx:JobCompetencyProfile a rdfs:Class ;
    dct:identifier "jdx:JobCompetencyProfile"@en-US ;
    rdfs:comment "Entity listing major work tasks, knowledge, skills, and abilities requirements for carrying out those work tasks based on a job analysis that engages subject matter experts in defining and validating the importance, frequency, or other types of weightings of the enumerated competencies."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Competency Profile"@en-US ;
    vann:usageNote "@@@"@en-US .
    
jdx:JobDescription a rdfs:Class ;
    dct:identifier "jdx:JobDescription"@en-US ;
    rdfs:comment "Entity providing a statement of a job position generally including the formal job title, duties, purpose, responsibilities, scope, reporting relationships and working conditions along with the knowledge, skills, abilities, education, qualifications, experience and other requirements for performing the job."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Description"@en-US ;
    vann:usageNote "@@@"@en-US .
    
jdx:JobOpening a rdfs:Class ;
    dct:identifier "jdx:JobOpening"@en-US ;
    rdfs:comment "Entity describing an unfilled job position that has been formally approved for filling; usually triggered by an authorization to a supervising/hiring manager to increase the number of positions or fill a position opened by people leaving."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Position Opening"@en-US ;
    vann:usageNote "@@@"@en-US .
    
jdx:JobPosting a rdfs:Class ;
    dct:identifier "jdx:JobPosting"@en-US ;
    rdfs:comment "Entity describing an on-line advertisement for filling one or more job openings"@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Posting"@en-US ;
    vann:usageNote "Entity that contains information on the employer and the job position based on a job description as well as how to apply for the job opening and how long the posting is valid."@en-US .
        
schema:MonetaryAmount a rdfs:Class ;
    dct:identifier "schema:MonetaryAmount"@en-US ;
    rdfs:comment "A monetary value or range."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Monetary Amount"@en-US ;
    vann:usageNote "This type can be used to describe an amount of money such as $50 USD, or a range as in describing a bank account being suitable for a balance between £1,000 and £1,000,000 GBP, or the value of a salary, etc."@en-US .
    
schema:Organization a rdfs:Class ;
    dct:identifier "schema:Organization"@en-US ;
    rdfs:comment "An organization such as a corporate body, employer, distribution channel provider"@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Organization"@en-US ;
    vann:usageNote "@@@"@en-US .
    
schema:Person a rdfs:Class ;
    dct:identifier "schema:Person"@en-US ;
    rdfs:comment "A human being regarded as an individual. (Oxford)"@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Person"@en-US ;
    vann:usageNote "@@@"@en-US .
    
schema:Place a rdfs:Class ;
    dct:identifier "schema:Place"@en-US ;
    rdfs:comment "A fixed physical location."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Location"@en-US ;
    vann:usageNote "@@@"@en-US .
    
schema:PostalAddress a rdfs:Class ;
    dct:identifier "schema:PostalAddress"@en-US ;
    rdfs:comment "The mailing address."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Postal Address"@en-US ;
    vann:usageNote ""@en-US .
    
jdx:PostingInstruction a rdfs:Class ;
    dct:identifier "jdx:PostingInstruction"@en-US ;
    rdfs:comment "An entity providing information about where and how a posting should be communicated."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Posting Instructions"@en-US ;
    vann:usageNote "A posting channel usually would be a job board or other advertising medium (HR)."@en-US .
    
schema:QuantitativeValueDistribution a rdfs:Class ;
    dct:identifier "schema:QuantitativeValueDistribution"@en-US ;
    rdfs:comment "A statistical distribution of values."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Quantitative Value Distribution"@en-US ;
    vann:usageNote ""@en-US .
    
jdx:ScaleAnnotation a rdfs:Class ;
    dct:identifier "jdx:ScaleAnnotation"@en-US ;
    rdfs:comment "Entity for identifying and applying a defined scale or rating to another entity."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Scale Annotation"@en-US ;
    vann:usageNote "Used, for example, when applying an importance scale to a competency identified in an instance of the AnnotatedDefinedTerm class."@en-US .
              
jdx:abilityType a rdfs:Property ;
    dct:identifier "jdx:abilityType"@en-US ;
    rdfs:comment "An aptitude, capacity or power to act physically, socially, mentally, legally, or morally at a level suitable to the work context;select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Ability Type"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range skos:ConceptScheme  ;
    vann:usageNote "[0..n] Use a value selected from a controlled vocabulary of ability types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US.
    
jdx:acquisitionDifficulty a rdfs:Property ;
    dct:identifier "jdx:acquisitionDifficulty"@en-US ;
    rdfs:comment "A rating or score representing the difficulty of the particular entity to acquire."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Acquisition Diffoculty"@en-US ;
    rdfs:domain jdx:ScaleAnnotation  ;
    rdfs:range xsd:float  ;
    vann:usageNote "[0..1] The scale may be applied to any appropriate controlled voabulary concept such as knowledge, skill, and ability as identified in the associated jdx:AnnotationDefinedTerm."@en-US .
    
jdx:activityType a rdfs:Property ;
    dct:identifier "jdx:activityType"@en-US ;
    rdfs:comment "An activity asssociated with the job."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Activity Type"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range skos:ConceptScheme  ;
    vann:usageNote "[0..n] Use a value selected from a controlled vocabulary of activity types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US; .
    
schema:address a rdfs:Property ;
    dct:identifier "schema:address"@en-US ;
    rdfs:comment "Physical address of the entity."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Address"@en-US ;
    rdfs:domain schema:Place, schema:Organization  ;
    rdfs:range schema:PostalAddress  ;
    vann:usageNote "[1]"@en-US .
    
schema:addressCountry a rdfs:Property ;
    dct:identifier "schema:addressCountry"@en-US ;
    rdfs:comment "The relavant country of the resource. For example, USA."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Country Code"@en-US ;
    rdfs:domain schema:PostalAddress  ;
    rdfs:range xsd:string  ;
    vann:usageNote "[1] Provide the two-letter ISO 3166-1 alpha-2 country code."@en-US .
    
schema:addressLocality a rdfs:Property ;
    dct:identifier "schema:addressLocality"@en-US ;
    rdfs:comment "The locality; e.g., Mountain View, London, Moscow."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "City"@en-US ;
    rdfs:domain schema:PostalAddress  ;
    rdfs:range xsd:string  ;
    vann:usageNote "[1] Name of the city, town, village, hamlet. "@en-US .
    
schema:addressRegion a rdfs:Property ;
    dct:identifier "schema:addressRegion"@en-US ;
    rdfs:comment "The region; e.g., CA, TX."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Address Region"@en-US ;
    rdfs:domain schema:PostalAddress  ;
    rdfs:range xsd:string  ;
    vann:usageNote "[1] The relevant state, province, territory or other subdivision of a nation state."@en-US .
    
jdx:annotation a rdfs:Property ;
    dct:identifier "jdx:annotation"@en-US ;
    rdfs:comment "Entity providing qualifying notations including application of scales and other weights to concepts and competencies identified through a jdx:AnnotatedDefinedTerm."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Related Annotation"@en-US ;
    rdfs:domain jdx:AnnnotatedDefinedTerm  ;
    rdfs:range jdx:ScaleAnnotation  ;
    vann:usageNote "[0-n] @@@"@en-US .
    
jdx:applicationCloseDate a rdfs:Property ;
    dct:identifier "jdx:applicationCloseDate"@en-US ;
    rdfs:comment "Closing date for accepting applications."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Application Close Data"@en-US ;
    rdfs:domain jdx:PostingInstructions  ;
    rdfs:range xsd:dateTime  ;
    vann:usageNote "[1] @@@"@en-US .
    
jdx:applicationContact a rdfs:Property ;
    dct:identifier "jdx:applicationContact"@en-US ;
    rdfs:comment "A job position contact point of a person or organization."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Contact Point"@en-US ;
    rdfs:domain jdx:PostingInstruction  ;
    rdfs:range schema:ContactPoint  ;
    vann:usageNote "[0-n] @@@"@en-US .
    
jdx:applicationInstructions a rdfs:Property ;
    dct:identifier "jdx:applicationInstructions"@en-US ;
    rdfs:comment "Narrative instructions for applying for this position."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Application Instructions"@en-US ;
    rdfs:domain jdx:PostingInstructions  ;
    rdfs:range rdf:langString  ;
    vann:usageNote "[1]"@en-US .
    
jdx:applicationLocation a rdfs:Property ;
    dct:identifier "jdx:applicationLocation"@en-US ;
    rdfs:comment "The physical location where job applications can be submitted."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Application Location"@en-US ;
    rdfs:domain jdx:PostingInstruction  ;
    rdfs:range schema:Place  ;
    vann:usageNote "[???]"@en-US .
    
jdx:assessmentMethod a rdfs:Property ;
    dct:identifier "jdx:assessmentMethod"@en-US ;
    rdfs:comment "Type of method used to conduct an assessment; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Assessment Method"@en-US ;
    rdfs:domain jdx:ScaleAnnotation  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[0..1] Use a value selected from a controlled vocabulary of assessment method types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US; .
    
jdx:attitudeType a rdfs:Property ;
    dct:identifier "jdx:attitudeType"@en-US ;
    rdfs:comment "Attitudes and ways of thinking necessary to successful performance of the job; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Attitude Type"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[0..n] Use a value selected from a controlled vocabulary of attitude types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US; .
    
schema:baseSalary a rdfs:Property ;
    dct:identifier "schema:baseSalary"@en-US ;
    rdfs:comment "The base salary of the job or of an employee in an employee role."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Base Salary@"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range schema:MonetaryAmount  ;
    vann:usageNote "[0...1]"@en-US .
    
jdx:careerLevelType a rdfs:Property ;
    dct:identifier "jdx:careerLevelType"@en-US ;
    rdfs:comment "A code classifying the level of a candidate based on career experience; select from an existing enumeration of such types. (HR)"@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Career Level Type"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[0..1] Use a value selected from a controlled vocabulary of career level types (e.g., HR Open's CareerLevelCodeList); encode using the jdx:AnnotatedDefinedTerm entity."@en-US .
    
schema:contactPoint a rdfs:Property ;
    dct:identifier "schema:contactPoint"@en-US ;
    rdfs:comment "A contact point for a person or organization."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Contact Point"@en-US ;
    rdfs:domain schema:Organization, jdx:PostingInstruction  ;
    rdfs:range schema:ContactPoint  ;
    vann:usageNote "[0-n]"@en-US .
    
schema:contactType a rdfs:Property ;
    dct:identifier "schema:contactType"@en-US ;
    rdfs:comment "Specification of the type of contact point."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Contact Type"@en-US ;
    rdfs:domain schema:ContactPoint  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[0..1] Use a value selected from a controlled vocabulary of contact types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .
    
jdx:credentialType a rdfs:Property ;
    dct:identifier "jdx:credentialRequirements"@en-US ;
    rdfs:comment "Type of credential such as badge, certification, bachelor degree; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Credential Type"@en-US ;
    rdfs:domain xsd:Credential  ;
    rdfs:range skos:Concept  ;
    owl:equivalentProperty: "http://purl.org/ctdl/terms/credentialType" ;
    vann:usageNote "[0..1] Use a value selected from a controlled vocabulary of credential types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:credentialRequirements a rdfs:Property ;
    dct:identifier "jdx:credentialRequirements"@en-US ;
    rdfs:comment "A credential required for the position."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Credential Requirements"@en-US ;
    rdfs:domain schema:ContactPoint  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[0..1] Use a value selected from a controlled vocabulary of contact types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

schema:datePosted a rdfs:Property ;
    dct:identifier "schema:datePosted"@en-US ;
    rdfs:comment "The date of posting by a channel."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:label "Date Posted"@en-US ;
    rdfs:domain jdx:JobPosition, jdx:JobPosting  ;
    rdfs:range xsd:dateTime  ;
    vann:usageNote "[0..1] @@@"@en-US .
    
schema:department a rdfs:Property ;
    dct:identifier "schema:department"@en-US ;
    rdfs:label "Department"@en-US ;
    rdfs:comment "A relationship between an organization and a department of that organization."@en-US ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:domain schema:Organization  ;
    rdfs:range schema:Organization  ;
    vann:usageNote "[0-n] @@@"@en-US .
    
schema:description a rdfs:Property ;
    dct:identifier "schema:description"@en-US ;
    rdfs:label "Description"@en-US ;    
    rdfs:comment "A description of the item."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:AnnotatedDefinedTerm, 
        "jdx:Education", 
        "jdx:Experience",
        "jdx:JobMaster", 
        "jdx:JobCompetencyProfile",
        "jdx:JobDescription", 
        "jdx:JobOpening", 
        "jdx:Credential", 
        "schema:MonetaryAmount", 
        "jdx:ScaleAnnotation"  ;
    rdfs:range rdf:langString  ;
    vann:usageNote "[1..n] @@@"@en-US .
    
jdx:distributionChannel a rdfs:Property ;
    dct:identifier "jdx:distributionChannel"@en-US ;
    rdfs:label "Distribution Channel@"@en-US ;    
    rdfs:comment "Identifies a job board or media channel associated with a job posting."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:postingInstructions  ;
    rdfs:range xsd:anyURI  ;
    vann:usageNote "[0-n] @@@"@en-US .
    
schema:duns a rdfs:Property ;
    dct:identifier "schema:duns"@en-US ;
    rdfs:label "DUNS"@en-US ;    
    rdfs:comment "Dun & Bradstreet DUNS number for identifying an organization or business person."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain schema:Organization  ;
    rdfs:range xsd:string  ;
    vann:usageNote "[0-1] @@@"@en-US .
    
schema:duration a rdfs:Property ;
    dct:identifier "schema:duration"@en-US ;
    rdfs:label "Duration"@en-US ;    
    rdfs:comment "The duration of the item (movie, audio recording, event, etc.) in ISO 8601 date format."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain schema:QuantitativeValueDistribution ;
    rdfs:range  xsd:string ;
    vann:usageNote "[1] Encode the duration value using the ISO 8601 data format."@en-US .
    
schema:educationLevel a rdfs:Property ;
    dct:identifier "schema:educationLevel"@en-US ;
    rdfs:label "Education Level"@en-US ;    
    rdfs:comment "A word or phrase identifying the location of the person in terms of his or her progression through an education or training context; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:Education, 
        "jdx:JobMaster"  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[0..1] Use a value selected from a controlled vocabulary of education levels such as ceterms:AudienceLevel or HR Open IscedCodeList; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .
    
schema:educationRequirement a rdfs:Property ;
    dct:identifier "schema:educationRequirement"@en-US ;
    rdfs:label "Education Requirement"@en-US ;    
    rdfs:comment "Educational background needed for the position or Occupation."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:JobMaster, 
        jdx:JobDescription, 
        jdx:JobOpening, 
        jdx:JobPosting  ;
    rdfs:range jdx:Education  ;
    vann:usageNote "[0-n] @@@"@en-US .
    
schema:email a rdfs:Property ;
    dct:identifier "schema:email"@en-US ;
    rdfs:label "Email Address"@en-US ;    
    rdfs:comment "Email address of an organization or person."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain schema:Organization, schema:ContactPoint  ;
    rdfs:range xsd:string  ;
    vann:usageNote "[1-n] @@@"@en-US .
    
schema:employmentType a rdfs:Property ;
    dct:identifier "schema:employmentType"@en-US ;
    rdfs:label "Emplyment Type"@en-US ;    
    rdfs:comment "Type of employment (e.g. full-time, part-time, contract, temporary, seasonal, internship); select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:JobMaster,
        jdx:JobDescription, 
        jdx:JobOpening, 
        jdx:JobPosting  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "[1] Use a value selected from a controlled vocabulary of education types such as full-time, part-time, contract, temporary, seasonal, internship; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .
    
jdx:employmentUnit a rdfs:Property ;
    dct:identifier "jdx:employmentUnit"@en-US ;
    rdfs:label "Employment Unit"@en-US ;    
    rdfs:comment "Department or unit of the employer where the employee reports and/or in which the job is to be performed."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:JobDescription,
        jdx:JobOpening, 
        jdx:JobPosting,
        jdx:JobMaster  ;
    rdfs:range schema:Organization, 
        schema:Department  ;
    vann:usageNote "[0-1] @@@"@en-US .
    
schema:estimatedSalary a rdfs:Property ;
    dct:identifier "schema:estimatedSalary"@en-US ;
    rdfs:label "Estimated Salary"@en-US ;    
    rdfs:comment "A property describing the estimated salary for a job posting based on a variety of variables including, but not limited to industry, job title, and location."@en-US ;
    dct:description "The estimated salary is usually computed by outside organizations and therefore the hiring organization is not bound to this estimated salary."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:JobMaster, jdx:JobMaster, jdx:JobOpening, jdx:JobPosting  ;
    rdfs:range schema:QualityValueDistribution  ;
    vann:usageNote "[0-1] @@@"@en-US .
    
jdx:evidence a rdfs:Property ;
    dct:identifier "jdx:evidence"@en-US ;
    rdfs:label "Evidence"@en-US ;    
    rdfs:comment "The referenced artifact or performance confirming or supporting an assertion."@en-US ;
