    rdfs:label "Special Commitment"@en-US ;    
    rdfs:comment "Any special commitments associated with this job (e.g. low-income, native American, ex-offenders, military commit) along with any special exclusions (e.g. criminal records)."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain jdx:JobMaster, jdx:JobPosting ;
    rdfs:range rdf:langString ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:streetAddress a rdfs:Property ;
    dct:identifier "schema:streetAddress"@en-US ;
    rdfs:label "Street Address"@en-US ;    
    rdfs:comment "The street address. For example, 1600 Amphitheatre Pkwy."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain schema:PostalAddress  ;
    rdfs:range rdf:langString ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:subOrganization a rdfs:Property ;
    dct:identifier "schema:subOrganization"@en-US ;
    rdfs:label "Sub-Organization"@en-US ;    
    rdfs:comment "A relationship between two organizations where the first includes the second, e.g., as a subsidiary."@en-US ;
    rdfs:isDefinedBy <https://schema.org> ;
    rdfs:domain schema:Organization  ;
    rdfs:range schema:Organization .

jdx:task a rdfs:Property ;
    dct:identifier "jdx:task"@en-US ;
    rdfs:label "Task"@en-US ;    
    rdfs:comment "A piece or unit of work to be done or undertaken and accomplished within a defined period of time or by a deadline; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:JobMaster, jdx:JobDescription, jdx:JobCompetenctProfile, jdx:JobPosting ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf jdx:competency ;
    vann:usageNote "Use a value selected from a controlled vocabulary of task types such as O*NET tasks; encode using the jdx:AnnotatedDefinedTerm entity."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:technology a rdfs:Property ;
    dct:identifier "jdx:technology"@en-US ;
    rdfs:label "Technology"@en-US ;    
    rdfs:comment "Tool, application or other form of technology that requires knowledge, skill and ability to use effectively in executing the tasks and responsibilities of the job; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:JobMaster, jdx:JobDescription, jdx:JobPosting ;
    rdfs:range skos:Concept  ;
    vann:usageNote "Use a value selected from a controlled vocabulary of technology types such as O*NET technology and tools; encode using the jdx:AnnotatedDefinedTerm entity."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:telephone a rdfs:Property ;
    dct:identifier "schema:telephone"@en-US ;
