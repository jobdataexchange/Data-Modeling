    rdfs:domain schema:Place, schema:ContactPoint, schema:Organization ;
    rdfs:range xsd:string ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:termCode a rdfs:Property ;
    dct:identifier "schema:termCode"@en-US ;
