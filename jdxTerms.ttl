@prefix dct: <http://purl.org/dc/terms/> .
@prefix jdx: <http://purl.org/jdx/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ceterms: <http://purl.org/ctdl/terms/> .
@prefix asn: <http://purl.org/asn/schema/core/> .
@prefix ceasn: <http://purl.org/ctdlasn/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

jdx:AnnotatedDefinedTerm a rdfs:Class ;
    dct:identifier "jdx:AnnotatedDefinedTerm"@en-US ;
    rdfs:comment "A word, name, acronym, phrase, defined in a controlled value space (e.g., taxonomy, glossary, dictionary, tag list, and other forms of enumeration)."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Annotated Defined Term"@en-US ;
    vann:usageNote "Used in the context of JDX to assign terms from occupation, industry and instructional program classifications, skills and abilities, and abstract concepts to JDX entities. Through the class' relationship with the jdx:ScaledAnnotation class, terms can be conditioned through applications of scales for attributes such as importance and frequency."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:AssessmentProfile a rdfs:Class ;
    dct:identifier "jdx:AssessmentProfile"@en-US ;
    rdfs:comment "Entity that describes the key characteristics of an assessment administered to candidates for a job."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Assessment Profile"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .    

jdx:Condition a rdfs:Class ;
    dct:identifier "jdx:Condition"@en-US ;
    rdfs:comment "Entity providing the means to make logical asserions that combine resources that in aggregate fulfill education, experience and credential requirements."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Condition"@en-US .    
    
schema:ContactPoint a rdfs:Class ;
    dct:identifier "schema:ContactPoint"@en-US ;
    rdfs:comment "A contact point—e.g., for information about a job opening or interview."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Contact Point"@en-US .
    
jdx:Credential a rdfs:Class ;
    dct:identifier "jdx:Credential"@en-US ;
    rdfs:comment "Entity describing authoritative permission to hold a certain status or to do certain things, e.g. to practice some trade or profession."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Credential"@en-US .    
  
schema:GeoCoordinates a rdfs:Class ;
    dct:identifier "schema:GeoCoordinates"@en-US ;
    rdfs:comment "Geographic coordinates of a place or event."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Location Geocoordinates"@en-US ;
    vann:usageNote "Use to identify a place using geo coordinates; preferably includes a URI for the location as well as longitude and latitude from a service such as GeoNames (https://www.geonames.org/)."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:JobCompetencyProfile a rdfs:Class ;
    dct:identifier "jdx:JobCompetencyProfile"@en-US ;
    rdfs:comment "Entity listing major work tasks, knowledge, skills, and abilities requirements for carrying out those work tasks based on a job analysis that engages subject matter experts in defining and validating the importance, frequency, or other types of weightings of the enumerated competencies."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Competency Profile"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:JobDescription a rdfs:Class ;
    dct:identifier "jdx:JobDescription"@en-US ;
    rdfs:comment "Entity providing a statement of a job position generally including the formal job title, duties, purpose, responsibilities, scope, reporting relationships and working conditions along with the knowledge, skills, abilities, education, qualifications, experience and other requirements for performing the job."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Description"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:JobMaster a rdfs:Class ;
    dct:identifier "jdx:JobMaster"@en-US ;
    rdfs:comment "Entity used internally as rdfs:domain or \"master\" to which all properties used in JobDescription, JobPosting, and JobCompetencyProfile are applicable. "@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Master"@en-US ;
    vann:usageNote "Do NOT use jdx:JobMaster in an entity in any public facing resource."@en-US .
    
jdx:JobPosting a rdfs:Class ;
    dct:identifier "jdx:JobPosting"@en-US ;
    rdfs:comment "Entity describing an on-line advertisement for filling one or more job openings."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Job Posting"@en-US ;
    vann:usageNote "Entity that contains information on the employer and the job position based on a job description as well as how to apply for the job opening and how long the posting is valid."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
            
schema:MonetaryAmountDistribution a rdfs:Class ;
    dct:identifier "schema:MonetaryAmountDistribution"@en-US ;
    rdfs:comment "Statistical distribution of monetary amounts."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Monetary Amount Distribution"@en-US .
        
schema:MonetaryAmount a rdfs:Class ;
    dct:identifier "schema:MonetaryAmount"@en-US ;
    rdfs:comment "A monetary value or range."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Monetary Amount"@en-US ;
    vann:usageNote "This type can be used to describe an amount of money such as $50 USD, or a range as in describing a bank account being suitable for a balance between £1,000 and £1,000,000 GBP, or the value of a salary, etc."@en-US .
    
schema:Organization a rdfs:Class ;
    dct:identifier "schema:Organization"@en-US ;
    rdfs:comment "An organization such as a corporate body, employer, distribution channel provider."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Organization"@en-US .
    
schema:Person a rdfs:Class ;
    dct:identifier "schema:Person"@en-US ;
    rdfs:comment "A human being regarded as an individual. (Oxford)"@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Person"@en-US .
    
schema:Place a rdfs:Class ;
    dct:identifier "schema:Place"@en-US ;
    rdfs:comment "A fixed physical location."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Location"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:PostalAddress a rdfs:Class ;
    dct:identifier "schema:PostalAddress"@en-US ;
    rdfs:comment "The mailing address of an entity."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Postal Address"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:PostingInstruction a rdfs:Class ;
    dct:identifier "jdx:PostingInstruction"@en-US ;
    rdfs:comment "An entity providing information about where and how a posting should be communicated."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Posting Instructions"@en-US ;
    vann:usageNote "A posting channel usually would be a job board or other advertising medium (HR)."@en-US .
        
jdx:QualifyingExperience a rdfs:Class ;
    dct:identifier "jdx:QualifyingExperience"@en-US ;
    rdfs:comment "Entity describing verifiable work, education or training experience supporting a person's qualification to perform the tasks and responsibilities of a position."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "QualifyingExperience"@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:QuantitativeValue a rdfs:Class ;
    dct:identifier "schema:QuantitativeValue"@en-US ;
    rdfs:comment "A point value or interval for product characteristics and other purposes."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Quantitative Value"@en-US .
    
jdx:Rubric a rdfs:Class ;
    dct:identifier "jdx:Rubric"@en-US ;
    rdfs:comment "Resosurce to define assessment criteria and their scoring"@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Rubric"@en-US .
    
jdx:ScaleAnnotation a rdfs:Class ;
    dct:identifier "jdx:ScaleAnnotation"@en-US ;
    rdfs:comment "Resource for identifying and applying a defined scale or rating to another resource."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Scale Annotation"@en-US ;
    vann:usageNote "Used, for example, when applying an importance scale to a competency identified in an instance of the AnnotatedDefinedTerm class."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
              
jdx:ability a rdfs:Property ;
    dct:identifier "jdx:ability"@en-US ;
    rdfs:comment "An aptitude, capacity or power to act physically, socially, mentally, legally, or morally at a level suitable to the work context;select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Ability"@en-US ;
    rdfs:subPropertyOf jdx:competency ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range skos:ConceptScheme  ;
    vann:usageNote "Use a value selected from a controlled vocabulary of ability types such as the O*NET cognitive, sensory, psychomotor and physical abilities; encode using the jdx:AnnotatedDefinedTerm entity."@en-US;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:acquiredInternally a rdfs:Property ;
    dct:identifier "jdx:acquiredInternally"@en-US ;
    rdfs:comment "Whether or not a requirement in a poosting may be acquired post-hire."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Acquired Internally"@en-US ;
    rdfs:domain jdx:ScaleAnnotation ;
    rdfs:range xsd:boolean .     
    
jdx:acquisitionDifficulty a rdfs:Property ;
    dct:identifier "jdx:acquisitionDifficulty"@en-US ;
    rdfs:comment "A rating or score representing the difficulty of the particular entity to acquire."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Acquisition Difficulty"@en-US ;
    rdfs:domain jdx:ScaleAnnotation  ;
    rdfs:range xsd:float  ;
    vann:usageNote "The scale may be applied to any appropriate controlled voabulary concept such as knowledge, skill, and ability as identified in the associated jdx:AnnotationDefinedTerm."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:address a rdfs:Property ;
    dct:identifier "schema:address"@en-US ;
    rdfs:comment "Physical address of the entity."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Address"@en-US ;
    rdfs:domain schema:Place, schema:Organization  ;
    rdfs:range schema:PostalAddress ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:addressCountry a rdfs:Property ;
    dct:identifier "schema:addressCountry"@en-US ;
    rdfs:comment "The relavant country of the resource. For example, USA."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Country Code"@en-US ;
    rdfs:domain schema:PostalAddress  ;
    rdfs:range xsd:string ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:addressLocality a rdfs:Property ;
    dct:identifier "schema:addressLocality"@en-US ;
    rdfs:comment "The locality; e.g., Mountain View, London, Moscow."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "City"@en-US ;
    rdfs:domain schema:PostalAddress ;
    rdfs:range xsd:string ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:addressRegion a rdfs:Property ;
    dct:identifier "schema:addressRegion"@en-US ;
    rdfs:comment "The region; e.g., CA, TX."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Address Region"@en-US ;
    rdfs:domain schema:PostalAddress  ;
    rdfs:range xsd:string  ;
    vann:usageNote "The relevant state, province, territory or other subdivision of a nation state."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .   
    
jdx:alternativeCondition a rdfs:Property ;
    dct:identifier "jdx:alternativeCondition"@en-US ;
    rdfs:label "Alternative Condition"@en-US ;    
    rdfs:comment "Resource referenced provides the means to make logical asserions about alternative conditions."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:Condition ;
    rdfs:range jdx:Condition ;
    vann:usageNote "USE IN PILOT"@en-US .
        
jdx:alternativeEducation a rdfs:Property ;
    dct:identifier "jdx:alternativeEducation"@en-US ;
    rdfs:label "Alternative Education"@en-US ;    
    rdfs:comment "Description of an alternative education fulfilling requirements for the position or occupation."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:Condition, jdx:JobMaster, jdx:JobDescription, jdx:jobPosting ;
    rdfs:range jdx:QualifyingExperience ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:alternativeExperience a rdfs:Property ;
    dct:identifier "jdx:alternativeExperience"@en-US ;
    rdfs:label "Alternative Experience"@en-US ;    
    rdfs:comment "Description of an alternative experience fulfilling requirements for the position or occupation."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:Condition, jdx:JobMaster, jdx:JobDescription, jdx:jobPosting ;
    rdfs:range jdx:QualifyingExperience ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:alternativeCredential a rdfs:Property ;
    dct:identifier "jdx:alternativeCredential"@en-US ;
    rdfs:label "Alternative Credential"@en-US ;    
    rdfs:comment "Description of an alternative credential fulfilling requirements for the position or occupation."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:Condition, jdx:JobMaster, jdx:JobDescription, jdx:jobPosting ;
    rdfs:range jdx:Credential ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:annotation a rdfs:Property ;
    dct:identifier "jdx:annotation"@en-US ;
    rdfs:comment "Entity providing qualifying notations including application of scales and other weights to concepts and competencies identified through a jdx:AnnotatedDefinedTerm."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Related Annotation"@en-US ;
    rdfs:domain jdx:AnnnotatedDefinedTerm  ;
    rdfs:range jdx:ScaleAnnotation ;
    vann:usageNote "USE IN PILOT"@en-US .
    
 jdx:annotatedTerm a rdfs:Property ;
    dct:identifier "jdx:annotatedTerm"@en-US ;
    rdfs:comment "The canonical term identified by the AnnotatedDefinedTerm"@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Annotated Term"@en-US ;
    rdfs:domain jdx:ScaleAnnotation, jdx:JobMaster; jdx:CompetencyProfile, jdx:JobDescription, jdx:JobPosting ;
    rdfs:range skos:Concept; ceasn:Competency; asn:Rubric  ;
    vann:usageNote "Use a value selected from a controlled vocabulary of types; likely external encounters with encodings in schema:DefinedTermSet, schema:DefinedTerm, W3C's SKOS, CTDL-ASN and ASN.."@en-US; 
    
jdx:applicationCloseDate a rdfs:Property ;
    dct:identifier "jdx:applicationCloseDate"@en-US ;
    rdfs:comment "Closing date for accepting applications."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Application Close Data"@en-US ;
    rdfs:domain jdx:PostingInstructions  ;
    rdfs:range xsd:dateTime .
    
jdx:applicationContact a rdfs:Property ;
    dct:identifier "jdx:applicationContact"@en-US ;
    rdfs:comment "Organization or person contact point for the posting."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Contact Point"@en-US ;
    rdfs:domain jdx:PostingInstruction  ;
    rdfs:range schema:ContactPoint .
    
jdx:applicationInstructions a rdfs:Property ;
    dct:identifier "jdx:applicationInstructions"@en-US ;
    rdfs:comment "Narrative instructions for applying for this position."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Application Instructions"@en-US ;
    rdfs:domain jdx:PostingInstructions  ;
    rdfs:range rdf:langString .
    
jdx:applicationLocation a rdfs:Property ;
    dct:identifier "jdx:applicationLocation"@en-US ;
    rdfs:comment "Physical location where job applications can be submitted."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Application Location"@en-US ;
    rdfs:domain jdx:PostingInstruction  ;
    rdfs:range schema:Place .
    
jdx:applicantLocationRequirement a rdfs:Property ;
    dct:identifier "jdx:applicantLocationRequirement"@en-US ;
    rdfs:comment "Geographic location(s) in which employees may be located for remote (telecommuting) jobs."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Applicant Location Requirement"@en-US ;
    rdfs:domain jdx:PostingInstruction  ;
    rdfs:range schema:GeoCoordinates ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:approvalStatusCode a rdfs:Property ;
    dct:identifier "jdx:approvalStatusCode"@en-US ;
    rdfs:comment "Values that describe the status of an approval process."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Approval Status Code"@en-US ;
    rdfs:domain jdx:JobMaster, jdx:JobDescription  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "Use a value selected from a controlled vocabulary of assessment method types such as the HR Open Standards ApprovalStatusCodeList.json; encode using the jdx:AnnotatedDefinedTerm entity."@en-US.
        
jdx:areaAssessed a rdfs:Property ;
    dct:identifier "jdx:areaAssessed"@en-US ;
    rdfs:label "Area Assessed"@en-US ;    
    rdfs:comment "Description of the area or domain to be assessed."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:domain jdx:AssessmentProfile ;
    rdfs:range rdf:langString .
        
jdx:assessment a rdfs:Property ;
    dct:identifier "jdx:assessment"@en-US ;
    rdfs:comment " Resource comprised of a direct, indirect, formative, and summative evaluation or estimation of the nature, ability, or quality of a person's performance or outcome of an action."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Assessment"@en-US ;
    rdfs:domain jdx:QualifyingExperience, jdx:Credential ;
    rdfs:range ceterms:Assessment ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:assessmentExample a rdfs:Property ;
    dct:identifier "jdx:assessmentExample"@en-US ;
    rdfs:comment "Resource describing an example assessment."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Assessment Example"@en-US ;
    rdfs:domain jdx:ScaleAnnotation ;
    rdfs:range xsd:anyURI ;
    vann:usageNote "USE IN PILOT"@en-US . 
 
jdx:assessmentRubric a rdfs:Property ;
    dct:identifier "jdx:assessmentRubric"@en-US ;
    rdfs:comment "Resource provided as assessment rubric for the annotated defined term ."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Assessment Ribric"@en-US ;
    rdfs:domain jdx:ScaleAnnotation ;
    rdfs:range xsd:anyURI ;
    vann:usageNote "USE IN PILOT"@en-US .
 
jdx:assessmentMethod a rdfs:Property ;
    dct:identifier "jdx:assessmentMethod"@en-US ;
    rdfs:comment "Type of method used to conduct an assessment; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Assessment Method"@en-US ;
    rdfs:domain jdx:ScaleAnnotation, jdx:AssessmentProfile  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "Use a value selected from a controlled vocabulary of assessment method types such as the ceterms:AssessmentMethod vocabulary; encode using the jdx:AnnotatedDefinedTerm entity."@en-US;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:attitude a rdfs:Property ;
    dct:identifier "jdx:attitude"@en-US ;
    rdfs:comment "Motivators of performance including values, aspirations, and priorities necessary to successful performance of a task or job; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Attitude"@en-US ;
    rdfs:domain jdx:JobMaster ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf jdx:competency ;
    vann:usageNote "Use a value selected from a controlled vocabulary of attitude types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
schema:baseSalary a rdfs:Property ;
    dct:identifier "schema:baseSalary"@en-US ;
    rdfs:comment "The base salary of the job or of an employee in an employee role including minimum and maximum salary and unit value (e.g., hour, day, week, month, or year)."@en-US ;
    rdfs:isDefinedBy <http://schema.org/> ;
    rdfs:label "Base Salary"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range schema:MonetaryAmount ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:careerLevel a rdfs:Property ;
    dct:identifier "jdx:careerLevel"@en-US ;
    rdfs:comment "A code classifying the level of a candidate based on career experience; select from an existing enumeration of such types."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Career Level"@en-US ;
    rdfs:domain jdx:JobMaster  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "Use a value selected from a controlled vocabulary of career level types (e.g., HR Open Standards CareerLevelCodeList.json); encode using the jdx:AnnotatedDefinedTerm entity."@en-US .
    
jdx:citizenshipRequirement a rdfs:Property ;
    dct:identifier "jdx:citizenshipRequirement"@en-US ;
    rdfs:comment "Required country of the applicant's citizenship."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Citizenship Requirement"@en-US ;
    rdfs:domain jdx:JobMaster, jdx:JobDescription, jdx:JobPosting ;
    rdfs:range skos:Concept  ;
    vann:usageNote "Use a value selected from a country code list such as the Library of Congress' List of Countries (http://id.loc.gov/vocabulary/countries.html).";  
    vann:usageNote "USE IN PILOT"@en-US .

jdx:competency a rdfs:Property ;
    dct:identifier "jdx:competency"@en-US ;
    rdfs:comment "Combination of observable and measurable knowledge, skills, abilities and personal attributes that contribute to enhanced employee performance; select from an existing enumeration of such types. (HR)"@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Competency"@en-US ;
    rdfs:domain jdx:JobMaster, jdx:JobDescription, jdx:JobPosting  ;
    rdfs:range skos:Concept  ;
    vann:usageNote "This property has been defined as a super-property for a set of more refined subproperties including knowledge, skills, abilities, attitudes, tasks, and work activities that indicate qualification for the job; encode competencyies using the jdx:AnnotatedDefinedTerm entity."@en-US ;
    vann:usageNote "USE IN PILOT"@en-US .
    
jdx:conceptKeyword a rdfs:Property ;
    dct:identifier "jdx:conceptKeyword"@en-US ;
    rdfs:comment "Word or phrase used to refine and describe significant aspects of the entity."@en-US ;
    rdfs:isDefinedBy <http://purl.org/jdx/terms/> ;
    rdfs:label "Concept Keyword"@en-US ;
    rdfs:domain xsd:AssessmentProfile  ;
    rdfs:range rdf:langString .

schema:contactPoint a rdfs:Property ;
    dct:identifier "schema:contactPoint"@en-US ;
    rdfs:comment "A contact point for a person or organization."@en-US ;
