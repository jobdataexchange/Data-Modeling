@prefix ceterms: <http://purl.org/ctdl/terms/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix jdx: <http://purl.org/jdx/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

jdx:AnnotatedDefinedTerm a rdfs:Class ;
    rdfs:label "Annotated Defined Term"@en-US ;
    dct:identifier "jdx:AnnotatedDefinedTerm" ;
    rdfs:comment "Word, name, acronym, phrase, defined in a controlled value space (e.g., taxonomy, glossary, dictionary, tag list, and other forms of enumeration)."@en-US ;
    rdfs:isDefinedBy jdx: ;
    skos:scopeNote "Used in the context of JDX to assign terms from occupation, industry and instructional program classifications, skills and abilities, and abstract concepts to JDX entities. Through the class' relationship with the jdx:ScaledAnnotation class, terms can be conditioned through applications of scales for attributes such as importance and frequency."@en-US .

jdx:AssessmentProfile a rdfs:Class ;
    rdfs:label "Assessment Profile"@en-US ;
    dct:identifier "jdx:AssessmentProfile" ;
    rdfs:comment "Entity that describes the key characteristics of an assessment administered to candidates for a job."@en-US ;
    rdfs:isDefinedBy jdx: .

jdx:Condition a rdfs:Class ;
    rdfs:label "Condition"@en-US ;
    dct:identifier "jdx:Condition"@en-US ;
    rdfs:comment "Entity providing the means to make logical asserions that combine resources that in aggregate fulfill education, experience and credential requirements."@en-US ;
    rdfs:isDefinedBy jdx: .

jdx:Credential a rdfs:Class ;
    rdfs:label "Credential"@en-US ;
    dct:identifier "jdx:Credential" ;
    rdfs:comment "Entity describing authoritative permission to hold a certain status or to do certain things, e.g. to practice some trade or profession."@en-US ;
    rdfs:isDefinedBy jdx: .

jdx:Job a rdfs:Class ;
    rdfs:label "Job"@en-US ;
    dct:identifier "jdx:Job" ;
    rdfs:comment "Formally defined and titled role for performing a set of work tasks or activities."@en-US ;
    rdfs:isDefinedBy jdx: ;
    skos:scopeNote "The jdx:JobDescription, jdx:JobCompetencyProfile and jdx:JobPosting define windows into the content of the jdx:Job; do NOT use jdx:Job as an entity in any public facing resource. Entity used internally as \"master\" to which all properties used in JobDescription, JobPosting, and JobCompetencyProfile are applicable."@en-US .

jdx:JobCompetencyProfile a rdfs:Class ;
    rdfs:label "Job Competency Profile"@en-US ;
    dct:identifier "jdx:JobCompetencyProfile" ;
    rdfs:comment "Entity listing major work tasks, knowledge, skills, and abilities requirements for carrying out those work tasks based on a job analysis that engages subject matter experts in defining and validating the importance, frequency, or other types of weightings of the enumerated competencies."@en-US ;
    rdfs:isDefinedBy jdx: .

jdx:JobDescription a rdfs:Class ;
    rdfs:label "Job Description"@en-US ;
    dct:identifier "jdx:JobDescription" ;
    rdfs:comment "Entity providing a statement of a job position generally including the formal job title, duties, purpose, responsibilities, scope, reporting relationships and working conditions along with the knowledge, skills, abilities, education, qualifications, experience and other requirements for performing the job."@en-US ;
    rdfs:isDefinedBy jdx: .

jdx:JobPosting a rdfs:Class ;
    rdfs:label "Job Posting"@en-US ;
    dct:description "Entity that contains information on the employer and the job position based on a job description as well as how to apply for the job opening and how long the posting is valid."@en-US ;
    dct:identifier "jdx:JobPosting" ;
    rdfs:comment "Entity describing an on-line advertisement for filling one or more job openings."@en-US ;
    rdfs:isDefinedBy jdx: .

jdx:PostingInstructions a rdfs:Class ;
    rdfs:label "Posting Instructions"@en-US ;
    dct:identifier "jdx:PostingInstructions" ;
    rdfs:comment "Entity providing information about where and how a posting should be communicated."@en-US ;
    rdfs:isDefinedBy jdx: ;
    skos:scopeNote "A posting channel usually would be a job board or other advertising medium (HR)."@en-US .

jdx:QualifyingExperience a rdfs:Class ;
    rdfs:label "QualifyingExperience"@en-US ;
    dct:identifier "jdx:QualifyingExperience" ;
    rdfs:comment "Entity describing verifiable work, education or training experience supporting a person's qualification to perform the tasks and responsibilities of a position."@en-US ;
    rdfs:isDefinedBy jdx: ;
    skos:scopeNote "A QualifyingExperience that results in the award of some form of credential should use the jdx:Credential class to describe that award."@en-US .

jdx:ScaleAnnotation a rdfs:Class ;
    rdfs:label "Scale Annotation"@en-US ;
    dct:identifier "jdx:ScaleAnnotation" ;
    rdfs:comment "Resource for identifying and applying a defined scale or rating to another resource."@en-US ;
    rdfs:isDefinedBy jdx: ;
    skos:scopeNote "Used, for example, when applying an importance scale to a competency identified in an instance of the AnnotatedDefinedTerm class."@en-US .

schema:ContactPoint a rdfs:Class ;
    rdfs:label "Contact Point"@en-US ;
    dct:identifier "schema:ContactPoint" ;
    rdfs:comment "Contact point—for example, for information about a job opening or interview."@en-US ;
    rdfs:isDefinedBy schema: .

schema:DefinedTermSet a rdfs:Class ;
    rdfs:label "Defined Term Set"@en-US ;
    dct:identifier "schema:DefinedTermSet" ;
    rdfs:comment "A set of defined terms for example a set of categories or a classification scheme, a glossary, dictionary or enumeration."@en-US ;
    rdfs:isDefinedBy schema: .

schema:GeoCoordinates a rdfs:Class ;
    rdfs:label "Location Geocoordinates"@en-US ;
    dct:identifier "schema:GeoCoordinates" ;
    rdfs:comment "Geographic coordinates of a place or event."@en-US ;
    rdfs:isDefinedBy schema: ;
    skos:scopeNote "Use to identify a place using geo coordinates; preferably includes a URI for the location as well as longitude and latitude from a service such as GeoNames (https://www.geonames.org/)."@en-US .

schema:MonetaryAmount a rdfs:Class ;
    rdfs:label "Monetary Amount"@en-US ;
    dct:identifier "schema:MonetaryAmount" ;
    rdfs:comment "A monetary value or range."@en-US ;
    rdfs:isDefinedBy schema: ;
    skos:scopeNote "This type can be used to describe an amount of money such as $50 USD, or a range as in describing a bank account being suitable for a balance between £1,000 and £1,000,000 GBP, or the value of a salary, etc."@en-US .

schema:MonetaryAmountDistribution a rdfs:Class ;
    rdfs:label "Monetary Amount Distribution"@en-US ;
    dct:identifier "schema:MonetaryAmountDistribution" ;
    rdfs:comment "Statistical distribution of monetary amounts."@en-US ;
    rdfs:isDefinedBy schema: .

schema:Organization a rdfs:Class ;
    rdfs:label "Organization"@en-US ;
    dct:identifier "schema:Organization"@en-US ;
    rdfs:comment "An organization such as a corporate body, employer, distribution channel provider."@en-US ;
    rdfs:isDefinedBy schema: .

schema:Place a rdfs:Class ;
    rdfs:label "Location"@en-US ;
    dct:identifier "schema:Place" ;
    rdfs:comment "Fixed physical location."@en-US ;
    rdfs:isDefinedBy schema: .

schema:PostalAddress a rdfs:Class ;
    rdfs:label "Postal Address"@en-US ;
    dct:identifier "schema:PostalAddress" ;
    rdfs:comment "Mailing address of an entity."@en-US ;
    rdfs:isDefinedBy schema: .

schema:PropertyValue a rdfs:Class ;
    rdfs:label "Property Value"@en-US ;
    dct:description "Entity provides the means to define a needed property not otherwise defined in the JDX schema or profile."@en-US ;
    dct:identifier "schema:PropertyValue" ;
    rdfs:comment "Property-value pair, e.g. representing a feature of an entity."@en-US ;
    rdfs:isDefinedBy schema: ;
    skos:scopeNote "Use PropertyValue sparingly! Always use a specific property from a known namespace when they serve your purpose. Using PropertyValue as a substitute for such properties will not trigger the same linked data function as the predicate of an RDF triple as will a more specific property."@en-US .

schema:QuantitativeValue a rdfs:Class ;
    rdfs:label "Quantitative Value"@en-US ;
    dct:identifier "schema:QuantitativeValue" ;
    rdfs:comment "Point value or interval for product characteristics and other purposes."@en-US ;
    rdfs:isDefinedBy schema: .

jdx:ability a rdfs:Property ;
    rdfs:label "Ability"@en-US ;
    dct:identifier "jdx:ability" ;
    rdfs:comment "Aptitude, capacity or power to act physically, socially, mentally, legally, or morally at a level suitable to the work context; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:Job ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    rdfs:subPropertyOf jdx:competency ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of ability types such as the O*NET cognitive, sensory, psychomotor and physical abilities; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:acquiredInternally a rdfs:Property ;
    rdfs:label "Acquired Internally"@en-US ;
    dct:identifier "jdx:acquiredInternally" ;
    rdfs:comment "Whether or not a requirement in a posting may be acquired post-hire."@en-US ;
    rdfs:domain jdx:ScaleAnnotation ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:boolean .

jdx:acquisitionDifficulty a rdfs:Property ;
    rdfs:label "Acquisition Difficulty"@en-US ;
    dct:identifier "jdx:acquisitionDifficulty" ;
    rdfs:comment "Rating or score representing the difficulty of the particular entity to acquire."@en-US ;
    rdfs:domain jdx:ScaleAnnotation ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:float ;
    skos:scopeNote "The scale may be applied to any appropriate controlled vocabulary concept such as knowledge, skill, and ability as identified in the associated jdx:AnnotationDefinedTerm."@en-US .

jdx:alternativeCondition a rdfs:Property ;
    rdfs:label "Alternative Condition"@en-US ;
    dct:identifier "jdx:alternativeCondition" ;
    rdfs:comment "Resource referenced provides the means to make logical asserions about alternative conditions."@en-US ;
    rdfs:domain jdx:Condition ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:Condition .

jdx:alternativeCredential a rdfs:Property ;
    rdfs:label "Alternative Credential"@en-US ;
    dct:identifier "jdx:alternativeCredential" ;
    rdfs:comment "Description of an alternative credential fulfilling requirements for the position or occupation."@en-US ;
    rdfs:domain jdx:Condition,
        jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:Credential .

jdx:alternativeEducation a rdfs:Property ;
    rdfs:label "Alternative Education"@en-US ;
    dct:identifier "jdx:alternativeEducation" ;
    rdfs:comment "Description of an alternative education or training fulfilling requirements for the position or occupation."@en-US ;
    rdfs:domain jdx:Condition,
        jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:QualifyingExperience .

jdx:alternativeExperience a rdfs:Property ;
    rdfs:label "Alternative Experience"@en-US ;
    dct:identifier "jdx:alternativeExperience" ;
    rdfs:comment "Description of an alternative experience fulfilling requirements for the position or occupation."@en-US ;
    rdfs:domain jdx:Condition,
        jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:QualifyingExperience .

jdx:annotation a rdfs:Property ;
    rdfs:label "Related Annotation"@en-US ;
    dct:identifier "jdx:annotation" ;
    rdfs:comment "Entity providing qualifying notations including application of scales and other weights to concepts and competencies identified through a jdx:AnnotatedDefinedTerm."@en-US ;
    rdfs:domain jdx:AnnotatedDefinedTerm ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:ScaleAnnotation .

jdx:applicantLocationRequirement a rdfs:Property ;
    rdfs:label "Applicant Location Requirement"@en-US ;
    dct:identifier "jdx:applicantLocationRequirement" ;
    rdfs:comment "Geographic location(s) in which employees may be located for remote (telecommuting) jobs."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range schema:GeoCoordinates .

jdx:applicationCloseDate a rdfs:Property ;
    rdfs:label "Application Close Data"@en-US ;
    dct:identifier "jdx:applicationCloseDate" ;
    rdfs:comment "Closing date for accepting applications."@en-US ;
    rdfs:domain jdx:PostingInstructions ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:dateTime .

jdx:applicationContact a rdfs:Property ;
    rdfs:label "Contact Point"@en-US ;
    dct:identifier "jdx:applicationContact" ;
    rdfs:comment "Organization or person contact point for the posting."@en-US ;
    rdfs:domain jdx:PostingInstructions ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range schema:ContactPoint .

jdx:applicationInstructions a rdfs:Property ;
    rdfs:label "Application Instructions"@en-US ;
    dct:identifier "jdx:applicationInstructions" ;
    rdfs:comment "Narrative instructions for applying for this position."@en-US ;
    rdfs:domain jdx:PostingInstructions ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range rdf:langString .

jdx:applicationLocation a rdfs:Property ;
    rdfs:label "Application Location"@en-US ;
    dct:identifier "jdx:applicationLocation" ;
    rdfs:comment "Physical location where job applications can be submitted."@en-US ;
    rdfs:domain jdx:PostingInstructions ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range schema:Place .

jdx:areaAssessed a rdfs:Property ;
    rdfs:label "Area Assessed"@en-US ;
    dct:identifier "jdx:areaAssessed" ;
    rdfs:comment "Description of the area or domain to be assessed."@en-US ;
    rdfs:domain jdx:AssessmentProfile ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range rdf:langString .

jdx:assessmentExample a rdfs:Property ;
    rdfs:label "Assessment Example"@en-US ;
    dct:identifier "jdx:assessmentExample" ;
    rdfs:comment "Resource describing an example assessment."@en-US ;
    rdfs:domain jdx:AssessmentProfile ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:anyURI .

jdx:assessmentMethod a rdfs:Property ;
    rdfs:label "Assessment Method"@en-US ;
    dct:identifier "jdx:assessmentMethod" ;
    rdfs:comment "Type of method used to conduct an assessment; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:AssessmentProfile,
        jdx:ScaleAnnotation ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of assessment method types such as the ceterms:AssessmentMethod vocabulary; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:attitude a rdfs:Property ;
    rdfs:label "Attitude"@en-US ;
    dct:identifier "jdx:attitude" ;
    rdfs:comment "Motivator of performance including values, aspirations, and priorities necessary to successful performance of a task or job; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:Job ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    rdfs:subPropertyOf jdx:competency ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of attitude types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:careerLevel a rdfs:Property ;
    rdfs:label "Career Level"@en-US ;
    dct:identifier "jdx:careerLevel" ;
    rdfs:comment "A code classifying the level of a candidate based on career experience; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:Job ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of career level types (e.g., HR Open Standards CareerLevelCodeList.json); encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:citizenshipRequirement a rdfs:Property ;
    rdfs:label "Citizenship Requirement"@en-US ;
    dct:identifier "jdx:citizenshipRequirement" ;
    rdfs:comment "Required country of the applicant's citizenship."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "Use a value selected from a country code list such as the Library of Congress' List of Countries (http://id.loc.gov/vocabulary/countries.html)."@en-US .

jdx:conceptKeyword a rdfs:Property ;
    rdfs:label "Concept Keyword"@en-US ;
    dct:identifier "jdx:conceptKeyword" ;
    rdfs:comment "Word or phrase used to refine and describe significant aspects of the entity."@en-US ;
    rdfs:domain jdx:AssessmentProfile ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range rdf:langString .

jdx:credentialCategory a rdfs:Property ;
    rdfs:label "Credential Category"@en-US ;
    dct:identifier "jdx:credentialCategory" ;
    rdfs:comment "Type of credential such as badge, certification, bachelor degree; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:Credential ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    owl:equivalentProperty: "http://purl.org/ctdl/terms/credentialType" ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of credential types (e.g., credential subclasses from CTDL); encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:derivedFrom a rdfs:Property ;
    rdfs:label "Derived From"@en-US ;
    dct:identifier "jdx:derivedFrom" ;
    rdfs:comment "Identifies a resource from which properties of the resource being described were obtained in whole or in part."@en-US ;
    rdfs:domain jdx:JobCompetencyProfile,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:Job .

jdx:disclaimer a rdfs:Property ;
    rdfs:label "Description"@en-US ;
    dct:identifier "jdx:disclaimer" ;
    rdfs:comment "Statement that denies aspects of a statement made or omitted by the employing entity in the position or posting."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range rdf:langString .

jdx:distributionChannel a rdfs:Property ;
    rdfs:label "Distribution Channel"@en-US ;
    dct:identifier "jdx:distributionChannel" ;
    rdfs:comment "Identifies a job board or media channel associated with a job posting."@en-US ;
    rdfs:domain jdx:PostingInstructions ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:anyURI .

jdx:educationLevel a rdfs:Property ;
    rdfs:label "Education Level"@en-US ;
    dct:identifier "schema:educationLevel" ;
    rdfs:comment "Word or phrase identifying the location of the person in terms of his or her progression through an education or training context; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting,
        jdx:QualifyingExperience ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of education levels such as ceterms:AudienceLevel or HR Open Standards IscedCodeList.json; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:employerOverview a rdfs:Property ;
    rdfs:label "Employer Overview"@en-US ;
    dct:identifier "jdx:employerOverview" ;
    rdfs:comment "Description of the employer, career opportunities and work environment."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobPosting ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:range rdf:langString .

jdx:employmentUnit a rdfs:Property ;
    rdfs:label "Employment Unit"@en-US ;
    dct:identifier "jdx:employmentUnit" ;
    rdfs:comment "Department, unit and/or facility where the employee reports and/or in which the job is to be performed."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range schema:Organization .

jdx:equivalentCredential a rdfs:Property ;
    rdfs:label "Equivalent Credential"@en-US ;
    dct:identifier "jdx:equivalentCredential" ;
    rdfs:comment "Description of an equivalent credential for the position or occupation."@en-US ;
    rdfs:domain jdx:Condition,
        jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:Credential .

jdx:equivalentEducation a rdfs:Property ;
    rdfs:label "Equivalent Education"@en-US ;
    dct:identifier "jdx:equivalentEducation" ;
    rdfs:comment "Description of equivalent education or training for the position or occupation."@en-US ;
    rdfs:domain jdx:Condition,
        jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:QualifyingExperience ;
    skos:scopeNote "An educational experience <u>without</u> award of a credential should be recorded with a range of jdx:QualifyingExperience. An educational experience <u>with</u> award of a credential should be recoded with a range of jdxCredential, not jdx:QualifyingExperience."@en-US .

jdx:equivalentExperience a rdfs:Property ;
    rdfs:label "Equivalent Experience"@en-US ;
    dct:identifier "jdx:equivalentEperience" ;
    rdfs:comment "Description of equivalent experience for the position or occupation."@en-US ;
    rdfs:domain jdx:Condition,
        jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:QualifyingExperience .

jdx:evidence a rdfs:Property ;
    rdfs:label "Evidence"@en-US ;
    dct:identifier "jdx:evidence" ;
    rdfs:comment "Referenced artifact or performance confirming or supporting an assertion."@en-US ;
    rdfs:domain jdx:Credential,
        jdx:QualifyingExperience ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:anyURI .

jdx:evidenceCategory a rdfs:Property ;
    rdfs:label "EvidenceType"@en-US ;
    dct:identifier "jdx:evidenceType" ;
    rdfs:comment "The type of referenced artifact, exam or performance confirming or supporting an assertion; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:AssessmentProfile,
        jdx:Credential,
        jdx:QualifyingExperience,
        jdx:ScaleAnnotation ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of evidence types; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:experienceCategory a rdfs:Property ;
    rdfs:label "Experience Category"@en-US ;
    dct:identifier "jdx:experienceCategory" ;
    rdfs:comment "Category of experience needed for the position or Occupation; select from an existing enumeration of such types."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting,
        jdx:QualifyingExperience ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "Use a value selected from a controlled vocabulary of experience types including, but not limited to, \"Education Experience\", \"Training Experience\", \"Work Experience\" and it's subtypes \"Apprenticeship\" and \"Traineeship\"; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

jdx:fein a rdfs:Property ;
    rdfs:label "FEIN"@en-US ;
    dct:identifier "jdx:fein" ;
    rdfs:comment "Federal Employer Identification Number (FEIN) identifying organizations, persons, states, government agencies, corporations, and companies."@en-US ;
    rdfs:domain schema:Organization ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range xsd:string ;
    owl:equivalentProperty: "http://purl.org/ctdl/terms/fein" .

jdx:formattedDescription a rdfs:Property ;
    rdfs:label "Formatted Description"@en-US ;
    dct:identifier "jdx:formattedDescription" ;
    rdfs:comment "HTML-tagged summary of the salient aspects of the job, including job responsibilities, qualifications, skills, working hours, education requirements, and experience requirements."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range rdf:HTML ;
    skos:scopeNote "For language-tagged descriptions without embedded HTML markup, use the schema:description property."@en-US .

jdx:hasCompetencyProfile a rdfs:Property ;
    rdfs:label "Has Competency Profile"@en-US ;
    dct:identifier "jdx:hasCompetencyProfile" ;
    rdfs:comment "Referenced resource describes the major work tasks, knowledge, skills, and abilities requirements for carrying out those work tasks based on a job analysis that engages subject matter experts in defining and validating the importance, frequency, or other types of weightings of the enumerated competencies."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:JobCompetencyProfile .

jdx:hasJob a rdfs:Property ;
    rdfs:label "Has Job Description"@en-US ;
    dct:identifier "jdx:hasJob" ;
    rdfs:comment "Entity referenced is a full description of a job defined and to be offered by the organization being described."@en-US ;
    rdfs:domain schema:Organization ;
