        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:range rdf:langString .

jdx:industryCode a rdfs:Property ;
    rdfs:label "Industry Code"@en-US ;
    dct:identifier "schema:industryCode" ;
    rdfs:comment "Code from a controlled vocabulary of industry types identifying the primary economic activity of an establishment defined as a single physical location where business is conducted or where services or industrial operations are performed."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting,
        schema:Organization ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "NOTE: In a JDX job description, there my be multiple levels and locations for an organization identified and each may have different industry codes based on the kinds of work performed at those locations; the NAICS industry code here in JDX applies SOLELY to the location where the work of the job being described will be performed."@en-US,
        "NOTE: Use a value selected from a controlled vocabulary of industry types used in the country where the job is located; e.g., NAICS in the U.S. and ESCO in the European Union; encode using the jdx:AnnotatedDefinedTerm entity."@en-US .

schema:name a rdfs:Property ;
    rdfs:label "Name"@en-US ;
    dct:identifier "schema:name" ;
    rdfs:comment "The name of the item."@en-US ;
    rdfs:domain jdx:AnnotatedDefinedTerm,
        jdx:JobCompetencyProfile,
        jdx:JobDescription,
        jdx:JobPosting,
        schema:ContactPoint,
        schema:DefinedTermSet,
        schema:GeoCoordinates,
        schema:Organization,
        schema:Place,
        schema:PostalAddress ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:range rdf:langString ;
    skos:scopeNote "NOTE: For the name of the hiring organization, use the subproperty legalName."@en-US .

schema:identifier a rdfs:Property ;
    rdfs:label "Position Identifier"@en-US ;
    dct:identifier "schema:identifier" ;
    rdfs:comment "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc."@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobCompetencyProfile,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy <https://schema.org/> ;
    rdfs:range schema:PropertyValue ;
    skos:scopeNote "NOTE: Identifier in JDX has a constrained range of schema:PropertyValue. Best practice is to use schema:name property to textually identify the type of identifier, schema:value property to hold the textual value of the identifier, and the schema;properytID for a URI indicating the type of identifier."@en-US,
        "NOTE: JDX defines subproperties called jdx:positionID and jdx:postingID to handle the employer's identifier for a particular position or posting."@en-US .

jdx:competency a rdfs:Property ;
    rdfs:label "Competency"@en-US ;
    dct:identifier "jdx:competency" ;
    rdfs:comment "Observable and measurable knowledge, skills, abilities and personal attributes that contribute to enhanced employee performance of the job; select from an existing enumeration of such types. (HR)"@en-US ;
    rdfs:domain jdx:Job,
        jdx:JobDescription,
        jdx:JobPosting ;
    rdfs:isDefinedBy jdx: ;
    rdfs:range jdx:AnnotatedDefinedTerm ;
    skos:scopeNote "This property has been defined as a super-property for a set of more refined subproperties including knowledge, skills, abilities, attitudes, tasks, and work activities that indicate qualification for the job; encode competencies using the jdx:AnnotatedDefinedTerm entity."@en-US .
